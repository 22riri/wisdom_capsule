<h1>Time Capsules</h1>

<% @timecapsules.each do |timecapsule| %>
  <div class="time-capsule">
    <h2><%= timecapsule.name %></h2>
    <p><%= timecapsule.created_at.strftime("%B %d, %Y") %></p>
  </div>
<% end %>
<div class= "btn btn-primary">
  <%= link_to 'Create', new_timecapsule_path %>
</div>
